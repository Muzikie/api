{
  "kind": "collectionType",
  "collectionName": "contribution_tiers",
  "info": {
    "singularName": "contribution-tier",
    "pluralName": "contribution-tiers",
    "displayName": "ContributionTier"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "minLength": 3
    },
    "description": {
      "type": "string",
      "minLength": 10,
      "maxLength": 140
    },
    "rewards": {
      "type": "string",
      "required": true,
      "minLength": 140
    },
    "amount": {
      "type": "biginteger",
      "required": true
    },
    "image": {
      "allowedTypes": [
        "images",
        "files"
      ],
      "type": "media",
      "multiple": true
    },
    "project": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::project.project",
      "inversedBy": "contribution_tiers"
    },
    "contributions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::contribution.contribution"
    },
    "exclusive_content": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::exclusive-content.exclusive-content",
      "inversedBy": "accessible_tiers"
    }
  }
}
